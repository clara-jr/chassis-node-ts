{
  "name": "chassis-node-ts",
  "version": "1.0.0",
  "description": "Node.js + Express.js + MongoDB API",
  "license": "ISC",
  "author": "Clara JimÃ©nez Recio",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "build": "npx tsc",
    "lint": "npx eslint . \"**/*.{js,ts,json}\"",
    "lint:fix": "npx eslint . \"**/*.{js,ts,json}\" --fix",
    "lint:quiet": "npx eslint . \"**/*.{js,ts,json}\" --quiet",
    "lint:staged": "npx eslint . --fix",
    "nodemon": "NODE_ENV=dev nodemon index.ts",
    "pre-commit": "npx lint-staged",
    "pre-commit-msg": "npx --no -- commitlint --edit ${1}",
    "pre-push": "NODE_ENV=test npx c8 --all mocha",
    "prepare": "husky",
    "start": "npx tsx ./index.ts",
    "start:dev": "NODE_ENV=dev npx tsx ./index.ts",
    "test": "NODE_ENV=test npx c8 --all mocha",
    "test:report": "NODE_ENV=test npx c8 --all --reporter=html -reporter=text mocha"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.23.0",
    "@types/chai": "^5.2.1",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mocha": "^10.0.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "c8": "^10.1.3",
    "chai": "^5.2.0",
    "eslint": "^9.23.0",
    "eslint-plugin-json-format": "^2.0.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "mocha": "^11.1.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.9",
    "supertest": "^7.1.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0"
  },
  "engines": {
    "node": ">=22.1.0",
    "npm": ">=10.7.0"
  }
}