{
  "name": "chassis-node-ts",
  "version": "1.0.0",
  "description": "Node.js + Express.js + MongoDB API",
  "license": "ISC",
  "author": "Clara JimÃ©nez Recio",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "build": "npx tsc",
    "lint": "npx eslint . --ext ts,json",
    "lint:fix": "npx eslint . --ext ts,json --fix",
    "lint:quiet": "npx eslint . --ext ts,json --quiet",
    "nodemon": "NODE_ENV=dev nodemon index.ts",
    "pre-commit": "npx lint-staged",
    "pre-commit-msg": "npx --no -- commitlint --edit ${1}",
    "pre-push": "NODE_ENV=test npx c8 --all mocha",
    "prepare": "husky",
    "start": "npx tsx ./index.ts",
    "start:dev": "NODE_ENV=dev npx tsx ./index.ts",
    "test": "NODE_ENV=test npx c8 --all mocha",
    "test:report": "NODE_ENV=test npx c8 --all --reporter=html -reporter=text mocha"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.2",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "mongoose": "^8.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@types/chai": "^4.3.12",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.20",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "c8": "^9.1.0",
    "chai": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-json-format": "^2.0.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mocha": "^10.3.0",
    "nodemon": "^3.1.0",
    "supertest": "^6.3.4",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.2.4"
  }
}
